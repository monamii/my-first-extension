{
    "name": "Drink water event popup",
    "description": "Demonstrates usage and features of the event page by reminding user to drink water",
    "version": "1.0",
    "manifest_version": 3,
    "background":{
        "service_worker": "background.js"
    },
    "permissions": ["storage", "alarms", "notifications"],
    "action":{
        "default_popup": "popup.html",
        "default_title": "Drink Water Event"
    },
    "icons": {
        "16": "images/drink_water16.png",
        "32": "images/drink_water32.png",
        "48": "images/drink_water48.png",
        "128": "images/drink_water128.png"
    }
}